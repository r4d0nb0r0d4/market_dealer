{
  "name": "market_dealer",
  "version": "1.0.0",
  "description": "The application for auto purchase/sell of skins",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [],
    "production": {
      "presets": [
        "minify"
      ]
    }
  },
  "prettier": {
    "semi": true,
    "printWidth": 120,
    "tabWidth": 2,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "requireConfig": true,
    "overrides": [
      {
        "files": [
          ",*.scss"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "keywords": [
    "CS-GO",
    "skins"
  ],
  "main": "index.js",
  "scripts": {
    "build-d": "webpack --config ./config/webpack.development.config.js",
    "build-p": "webpack --config ./config/webpack.production.config.js",
    "pr-d": "npm run build-d",
    "pr-p": "npm run build-p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r4d0nb0r0d4/market_dealer.git"
  },
  "author": "Radislav Erokho <radislav-erokho@yandex.by>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/r4d0nb0r0d4/market_dealer/issues"
  },
  "homepage": "https://github.com/r4d0nb0r0d4/market_dealer#readme",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.25",
    "@types/node-fetch": "^2.5.8",
    "@types/webpack": "^4.41.26",
    "babel-loader": "^8.2.2",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "tweetnacl": "^1.0.3"
  }
}
